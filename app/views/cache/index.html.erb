CACHE MANIFEST
<% paths_for_manifests(%w(application.js)).each do |manifest_path| %>
<%= "/assets/#{@environment.find_asset(manifest_path).digest_path}" rescue nil %>
<% end %>
<% @asset_list.each do |asset| %>
<%= "/assets/#{@environment.find_asset(asset).digest_path}" rescue nil %>
<% end %>
<%= stylesheet_path 'application' %>
<%= javascript_path 'application' %>
<% @items.each do |item| %>
<%= item_path item %>
<%= image_item_path(item, format: :jpg) %>
<%= audio_item_path(item, format: :wav) %>
<% end %>

